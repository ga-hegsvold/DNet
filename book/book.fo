adding-start="2px" padding-end="2px" padding-top="2px" padding-bottom="2px" border-bottom-width="0.5pt" border-bottom-style="solid" border-bottom-color="black" border-right-width="0.5pt" border-right-style="solid" border-right-color="black"><fo:block><fo:inline font-style="italic">40</fo:inline></fo:block></fo:table-cell><fo:table-cell padding-start="2px" padding-end="2px" padding-top="2px" padding-bottom="2px" border-bottom-width="0.5pt" border-bottom-style="solid" border-bottom-color="black" border-right-width="0.5pt" border-right-style="solid" border-right-color="black"><fo:block><fo:inline font-style="italic">8 * Base</fo:inline></fo:block></fo:table-cell><fo:table-cell padding-start="2px" padding-end="2px" padding-top="2px" padding-bottom="2px" border-bottom-width="0.5pt" border-bottom-style="solid" border-bottom-color="black"><fo:block><fo:inline font-style="italic">(7)</fo:inline></fo:block></fo:table-cell></fo:table-row><fo:table-row><fo:table-cell padding-start="2px" padding-end="2px" padding-top="2px" padding-bottom="2px" border-right-width="0.5pt" border-right-style="solid" border-right-color="black"><fo:block><fo:inline font-style="italic">Max**</fo:inline></fo:block></fo:table-cell><fo:table-cell padding-start="2px" padding-end="2px" padding-top="2px" padding-bottom="2px" border-right-width="0.5pt" border-right-style="solid" border-right-color="black"><fo:block><fo:inline font-style="italic">36</fo:inline></fo:block></fo:table-cell><fo:table-cell padding-start="2px" padding-end="2px" padding-top="2px" padding-bottom="2px" border-right-width="0.5pt" border-right-style="solid" border-right-color="black"><fo:block><fo:inline font-style="italic">60</fo:inline></fo:block></fo:table-cell><fo:table-cell padding-start="2px" padding-end="2px" padding-top="2px" padding-bottom="2px" border-right-width="0.5pt" border-right-style="solid" border-right-color="black"><fo:block><fo:inline font-style="italic">12 * Base</fo:inline></fo:block></fo:table-cell><fo:table-cell